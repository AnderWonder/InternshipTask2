<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhizhkin.andrey.internshiptask2">

    <application
        android:name=".EcontactApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RequestsManager.RequestsManagerActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RequestViewer.RequestViewerActivity"
            android:label="@string/request_viewer_request_id"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RequestsManager.RequestsManagerActivity">

        </activity>
    </application>

</manifest>